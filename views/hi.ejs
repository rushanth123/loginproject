<% units.forEach(function (units) { %>
    <div style="border:3px solid red">
    <li>
        <p><%= units.title %></p>
          <iframe width="900" height="200" src="<%= units.urlid %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        <p><%= units.subid %></p>
        <form action="/del-units" method="post">
          <input type="hidden" name="unit_id" value="<%= units._id%>">
          <input type="submit" value="Delete">
        </form>
        <!-- <iframe width="420" height="315" src="">
  </iframe> -->


    </div>
<% }) %>
